{
  "name": "action-culture-backend",
  "version": "1.0.0",
  "description": "Backend API pour Action Culture",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:safe": "node startWithChecks.js",
    "redis:dev": "node scripts/redis-dev.js",
    "dev": "concurrently \"npm run redis:dev\" \"npm run start:dev\"",
    "dev:safe": "nodemon startWithChecks.js",
    "test": "jest",
    "test:routes": "node testAllRoutes.js",
    "generate:secret": "node generateSecret.js",
    "setup": "npm run setup:env && npm run generate:secret && npm run setup:uploads",
    "setup:env": "cp .env.example .env || copy .env.example .env",
    "setup:uploads": "node setupUploads.js",
    "clean:temp": "node cleanTempUploads.js",
    "db:check": "node -e \"require('./startWithChecks.js')\"",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "db:init": "node scripts/init-database.js init",
    "db:reset": "node scripts/init-db.js reset --force",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix"
  },
  "keywords": [
    "api",
    "culture",
    "algeria",
    "express",
    "sequelize"
  ],
  "author": "Action Culture Team",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^9.8.0",
    "axios": "^1.9.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.3",
    "bull": "^4.16.5",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.6.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-list-endpoints": "^7.1.1",
    "express-rate-limit": "^6.11.2",
    "express-slow-down": "^2.1.0",
    "express-validator": "^7.0.1",
    "faker-js": "^1.0.0",
    "form-data": "^4.0.3",
    "handlebars": "^4.7.8",
    "helmet": "^7.0.0",
    "ioredis": "^5.6.1",
    "jimp": "^1.6.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.2",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.3",
    "nodemailer": "^7.0.11",
    "pdfkit": "^0.17.1",
    "qrcode": "^1.5.4",
    "rate-limit-redis": "^4.2.1",
    "redis": "^5.5.6",
    "sequelize": "^6.33.0",
    "sharp": "^0.34.2",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "redis-memory-server": "^0.15.0",
    "sequelize-cli": "^6.6.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  }
}
